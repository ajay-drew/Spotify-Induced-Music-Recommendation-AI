# SIMRAI Configuration Template
# Copy this file to .env and fill in your actual values

# ============================================================================
# SPOTIFY API CREDENTIALS (Required)
# ============================================================================
SIMRAI_SPOTIFY_CLIENT_ID=your_spotify_client_id_here
SIMRAI_SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here
SIMRAI_SPOTIFY_REDIRECT_URI=http://localhost:8000/auth/callback

# ============================================================================
# GROQ API KEY (Optional - for AI-enhanced mood interpretation)
# ============================================================================
GROQ_API_KEY=your_groq_api_key_here
SIMRAI_GROQ_MODEL=llama-3.1-8b-instant

# ============================================================================
# LOGGING (Optional)
# ============================================================================
SIMRAI_LOG_LEVEL=INFO
